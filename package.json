{
  "name": "@se/web-ui",
  "version": "3.2.1",
  "private": true,
  "description": "Library of HTML [custom element](https://developers.google.com/web/fundamentals/web-components/customelements) that can be used with any framework like jquery, React, Vue or even no framework at all.",
  "scripts": {
    "start": "stencil build --dev --watch --serve --port 3344",
    "postinstall": "npm-run-all install.*",
    "install.core": "cd src-core && yarn",
    "install.angular": "cd src-angular && yarn",
    "build": "npm-run-all build.*",
    "build.core": "cd src-core && yarn build",
    "build.angular": "cd src-angular && yarn build",
    "release": "npm-run-all release.*",
    "release.publish": "node publish.js"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "license": "MIT",
  "dependencies": {
    "execa": "^1.0.0",
    "fs-extra": "^7.0.1",
    "listr": "^0.14.3",
    "turbocolor": "^2.6.1"
  }
}
