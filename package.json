{
  "name": "@se/web-ui",
  "version": "3.5.4",
  "private": true,
  "description": "Library of HTML [custom element](https://developers.google.com/web/fundamentals/web-components/customelements) that can be used with any framework like jquery, React, Vue or even no framework at all.",
  "scripts": {
    "start": "stencil build --dev --watch --serve --port 3344",
    "postinstall": "npm-run-all install.*",
    "install.core": "cd src-core && yarn",
    "install.angular": "cd src-angular && yarn",
    "install.react": "cd src-react && yarn",
    "prebuild": "npm-run-all prebuild.*",
    "prebuild.rm.core": "rimraf \"./core\"",
    "prebuild.rm.angular": "rimraf \"./angular\"",
    "prebuild.rm.react": "rimraf \"./react\"",
    "build": "npm-run-all build.*",
    "build.core": "cd src-core && yarn build",
    "build.angular": "cd src-angular && yarn build",
    "build.react": "cd src-react && yarn build",
    "test": "npm-run-all test.*",
    "test.core": "cd src-core && yarn test",
    "test.angular": "cd src-angular && yarn test",
    "release": "node release.js"
  },
  "devDependencies": {
    "husky": "^4.2.1",
    "npm-run-all": "^4.1.5"
  },
  "license": "MIT",
  "dependencies": {
    "@se/icons": "^3.5.3",
    "colorette": "^1.1.0",
    "execa": "^3.2.0",
    "fs-extra": "^8.1.0",
    "listr": "^0.14.3",
    "rimraf": "^3.0.0",
    "turbocolor": "^2.6.1"
  }
}
