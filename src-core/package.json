{
  "name": "@se/web-ui",
  "version": "0.0.1-PLACEHOLDER",
  "private": true,
  "description": "Library of HTML [custom element](https://developers.google.com/web/fundamentals/web-components/customelements) that can be used with any framework like jquery, React, Vue or even no framework at all.",
  "module": "core/index.mjs",
  "main": "core/index.js",
  "types": "core/types/components.d.ts",
  "collection": "core/collection/collection-manifest.json",
  "scripts": {
    "start": "stencil build --dev --watch --serve --port 3344",
    "postinstall": "yarn build",
    "build": "npm-run-all build.*",
    "build.core": "stencil build --docs",
    "build.localRef": "cpx \"src/package.json\" core",
    "build.export.theme": "cpx \"src/theme/**/*\" core/styles",
    "build.export.docs": "cpx \"src/**/*.md\" core/docs",
    "build.move.core": "cpx \"core/**/*\" \"../core/\"",
    "build.rm.core": "rimraf \"./core\"",
    "test": "stencil test --e2e",
    "spec": "stencil test --spec --coverage",
    "e2e": "stencil test --e2e",
    "e2e.screenshot": "stencil test --e2e --screenshot",
    "test.watch": "stencil test --e2e --watchAll",
    "spec.watch": "stencil test --spec --watchAll"
  },
  "dependencies": {
    "@se/fonts": "^1.0.0",
    "@se/icons": "^3.4.0",
    "@stencil/core": "^1.3.0",
    "@stencil/sass": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/puppeteer": "^1.19.1",
    "cpx": "^1.5.0",
    "http-server": "^0.11.1",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "nodecat": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "pixelmatch": "^5.0.2",
    "puppeteer": "1.19.0",
    "rimraf": "^3.0.0",
    "workbox-build": "^4.3.1"
  },
  "license": "MIT"
}
