{
  "name": "react-angular-elements",
  "version": "0.1.0",
  "private": true,
  "homepage" : "https://pages.github.schneider-electric.com/IoT/ui-web-components",
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "postbuild": "cpx \"build/**/*\" \"../docs\"",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "lint": "react-app-rewired run lint",
    "upgrade:web": "yarn upgrade @se/ui-web-components",
    "postinstall": "vendor-copy"
  },
  "dependencies": {
    "@se/fonts": "^1.0.0",
    "@se/icons": "^3.0.4",
    "@se/ui-web-components": "file:../core",
    "@vaadin/vaadin-split-layout": "^4.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.7",
    "monaco-editor-webpack-plugin": "^1.7.0",
    "react": "^16.6.1",
    "react-app-rewired": "^2.1.0",
    "react-dom": "^16.6.1",
    "react-monaco-editor": "^0.24.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "vendor-copy": "^2.0.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "vendorCopy": [
    {
      "from": "node_modules/@webcomponents/webcomponentsjs/custom-elements-es5-adapter.js",
      "to": "public/vendor/custom-elements-es5-adapter.js"
    },
    {
      "from": "node_modules/@webcomponents/webcomponentsjs/webcomponents-bundle.js",
      "to": "public/vendor/webcomponents-bundle.js"
    }
  ]
}
