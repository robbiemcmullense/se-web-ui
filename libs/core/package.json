{
  "name": "@se/web-ui",
  "version": "3.6.1",
  "description": "web-ui library of web-component to build SE application",
  "main": "index.js",
  "module": "index.mjs",
  "es2015": "esm/index.mjs",
  "es2017": "esm/index.mjs",
  "types": "types/components.d.ts",
  "collection": "collection/collection-manifest.json",
  "collection:main": "collection/index.js",
  "unpkg": "se-components/se-components.js",
  "publishConfig": {
    "registry": "https://nexus.tools.struxurewarecloud.com/repository/npm-internal/"
  },
  "scripts": {
    "build": "npm-run-all build.*",
    "build.core": "stencil build --docs --prod",
    "build.export.docs": "cpx \"src/**/*.md\" \"../../dist/libs/core/docs\"",
    "build.export.types": "cpx \"types/*\" \"../../dist/libs/core/types\"",
    "build.rm.types": "rimraf \"types\"",
    "build_.dash": "node scripts/index.js",
    "test": "stencil test --e2e --testTimeout=30000",
    "posttest": "stencil test --spec --coverage",
    "spec": "stencil test --spec",
    "e2e": "stencil test --e2e",
    "e2e.screenshot": "stencil test --e2e --screenshot",
    "test.watch": "stencil test --e2e --watchAll",
    "spec.watch": "stencil test --spec --watchAll",
    "watch": "npm run build && stencil build --watch"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "24.0.25",
    "@types/puppeteer": "1.20.3",
    "jest": "24.9.0",
    "puppeteer": "1.20.0"
  }
}
