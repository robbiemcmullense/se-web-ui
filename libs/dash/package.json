{
    "name": "web-ui-dash",
    "version": "0.0.1-PLACEHOLDER",
    "description": "Python Dash library to extend web-ui library components",
    "main": "build/index.js",
    "scripts": {
        "prestart": "npm run custom.build.all.copy.src",
        "start": "webpack-serve --config ./webpack.serve.config.js --open",
        "validate-init": "python _validate_init.py",
        "prepublishOnly": "npm run validate-init",
        "build:js": "webpack --mode production",
        "build:backends": "dash-generate-components ./src/lib/components web_ui_dash -p package-info.json --r-prefix se --jl-prefix se --ignore \\.test\\.",
        "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
        "build": "npm run build:custom && npm run build:js && npm run build:backends",
        "build:activated": "npm run build:custom && npm run build:js && npm run build:backends-activated",
        "build:custom": "npm-run-all custom.build.all.*",
        "custom.build.all.copy.fonts-styles": "cpx \"./node_modules/@se/fonts/css/*\" assets/fonts/css",
        "custom.build.all.copy.fonts-fonts": "cpx \"./node_modules/@se/fonts/fonts/*\" assets/fonts/fonts",
        "custom.build.all.copy.icons-styles": "cpx \"./node_modules/@se/icons/css/*\" assets/icons/css",
        "custom.build.all.copy.icons-fonts": "cpx \"./node_modules/@se/icons/fonts/*\" assets/icons/fonts",
        "custom.build.all.copy.se-components": "cpx \"./node_modules/@se/web-ui/se-components/*\" assets/se-components",
        "custom.build.all.copy.init": "cpx \"./templates/*\" web_ui_dash"
    },
    "author": "Francois Delpech <francois.delpech@se.com>",
    "license": "MIT",
    "dependencies": {
        "@se/fonts": "^1.2.0",
        "@se/icons": "^4.9.0",
        "@se/web-ui": "^3.25.1",
        "ramda": "^0.26.1"
    },
    "devDependencies": {
        "@babel/core": "^7.5.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.4",
        "@babel/preset-env": "^7.5.4",
        "@babel/preset-react": "^7.0.0",
        "@plotly/dash-component-plugins": "^1.2.0",
        "@plotly/webpack-dash-dynamic-import": "^1.2.0",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^8.0.6",
        "copyfiles": "^2.1.1",
        "cpx": "^1.5.0",
        "css-loader": "^3.0.0",
        "eslint": "^6.0.1",
        "eslint-config-prettier": "^6.0.0",
        "eslint-plugin-import": "^2.18.0",
        "eslint-plugin-react": "^7.14.2",
        "npm-run-all": "^4.1.5",
        "prop-types": "^15.7.2",
        "react": "^16.8.6",
        "react-docgen": "^4.1.1",
        "react-dom": "^16.8.6",
        "style-loader": "^0.23.1",
        "styled-jsx": "^3.2.1",
        "terser-webpack-plugin": "^2.3.0",
        "webpack": "4.36.1",
        "webpack-cli": "3.3.6",
        "webpack-serve": "3.1.0"
    },
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=6.1.0"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ]
    }
}
