{
  "name": "web-ui-dash",
  "version": "0.0.1",
  "description": "Python Dash library to extend web-ui library components",
  "main": "build/index.js",
  "scripts": {
    "prestart": "npm run build.all.copy.src",
    "start": "webpack-serve ./webpack.serve.config.js --open",
    "build": "npm-run-all build.all.*",
    "build.all.copy.src": "cpx \"../src-react/src/**/*\" src",
    "build.all.copy.fonts-styles": "cpx \"./node_modules/@se/fonts/css/*\" assets/fonts/css",
    "build.all.copy.fonts-fonts": "cpx \"./node_modules/@se/fonts/fonts/*\" assets/fonts/fonts",
    "build.all.copy.icons-styles": "cpx \"./node_modules/@se/icons/css/*\" assets/icons/css",
    "build.all.copy.icons-fonts": "cpx \"./node_modules/@se/icons/fonts/*\" assets/icons/fonts",
    "build.all.copy.init": "cpx \"./templates/*\" web_ui_dash",
    "build.all.js": "webpack --mode production",
    "build.all.py": "dash-generate-components \"./src/components\" web_ui_dash",
    "build.all.rm.src": "rimraf \"./src\"",
    "build.js": "webpack --mode development",
    "build.activated.py": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py)",
    "build.activated": "(. venv/bin/activate || venv\\scripts\\activate && npm-run-all build)"
  },
  "author": "Francois Delpech francois.delpech@se.com",
  "license": "MIT",
  "dependencies": {
    "@se/fonts": "^1.0.0",
    "@se/icons": "^3.0.4",
    "@se/web-ui": "^2.0.0",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "copyfiles": "^2.0.0",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-react": "^7.9.1",
    "npm": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "react": ">=0.14",
    "react-docgen": "^2.20.1",
    "react-dom": ">=0.14",
    "rimraf": "^2.6.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.1",
    "webpack-serve": "^1.0.2"
  },
  "engines": {
    "node": ">=8.11.0",
    "npm": ">=6.1.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
